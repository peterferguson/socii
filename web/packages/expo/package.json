    "@types/node": "^16.10.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "expo-cli": "^4.12.10",
    "next-compose-plugins": "^2.2.1",
    "typescript": "~4.3.5"
  },
  "private": true,
  "version": "1.0.0",
