{
  "build": {
    "release": {
      "releaseChannel": "production",
      "ios": {
        "cocoapods": "1.11.2"
      },
      "env": {
        "STAGE": "production",
        "NEXT_PUBLIC_FIREBASE_PROJECT_ID": "socii"
      }
    },
    "development": {
      "extends": "release",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "STAGE": "development",
        "NEXT_PUBLIC_FIREBASE_PROJECT_ID": "socii-development"
      }
    }
  },
  "cli": {
    "version": ">= 0.34.1"
  }
}
